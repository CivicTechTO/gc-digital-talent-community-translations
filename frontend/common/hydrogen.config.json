  ],
  "containers": [
    {
      "key": "s",
      "maxWidth": "39rem"
    },
    {
      "key": "m",
      "maxWidth": "60rem"
    },
    {
      "key": "l",
      "maxWidth": "80rem"
    },
    {
      "key": "full",
      "maxWidth": "none"
    }
  ],
  "fonts": [
    {
      "key": "sans",
      "family": "'Open Sans', sans-serif"
    },
    {
      "key": "serif",
      "family": "serif"
    },
    {
      "key": "script",
      "family": "script"
    },
    {
      "key": "mono",
      "family": "monospace"
    }
  ],
  "gradients": [
    {
      "key": "dt-linear",
      "gradient": "linear-gradient(#674C90, #1D2C4C)",
      "fallback": "#674C90"
    },
    {
      "key": "ia-linear-primary",
      "gradient": "linear-gradient(#272F6B, #0F1037)",
      "fallback": "#272F6B"
    },
    {
      "key": "ia-linear-secondary",
      "gradient": "linear-gradient(#C01E5A, #781443)",
      "fallback": "#C01E5A"
    }
  ],
  "media": [
    {
      "key": "b",
      "query": null
    },
    {
      "key": "p",
      "query": "print"
    },
    {
      "key": "s",
      "query": "screen and (min-width: 48em)"
    },
    {
      "key": "m",
      "query": "screen and (min-width: 64em)"
    },
    {
      "key": "l",
      "query": "screen and (min-width: 100em)"
    }
  ],
  "radius": [
    {
      "key": "s",
      "radius": "10px"
    },
    {
      "key": "m",
      "radius": "50rem"
    },
    {
      "key": "l",
      "radius": "100%"
    }
  ],
  "shadows": [
    {
      "key": "s",
      "shadow": "0 0.1rem 0.2rem 0 rgba(0, 0, 0, .2)"
    },
    {
      "key": "m",
      "shadow": "0 0.25rem 0.5rem -0.05rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "l",
      "shadow": "0 0.4rem 0.7rem -0.1rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "xl",
      "shadow": "0 0.55rem 1rem -0.2rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "xxl",
      "shadow": "0 0.7rem 1.5rem -0.3rem rgba(0, 0, 0, .2)"
    }
  ],
  "transitions": {
    "durations": [
      {
        "key": "default",
        "duration": ".2s"
      }
    ],
    "functions": [
      {
        "key": "ease",
        "function": "ease"
      }
    ],
    "delays": [
      {
        "key": "none",
        "delay": "0s"
      }
    ]
  },
  "typography": [
    {
      "size": "100%",
      "lineHeight": "1.3",
      "queryKey": "b",
      "typeScale": "1.25"
    },
    {
      "size": "112.5%",
      "lineHeight": "1.3",
      "queryKey": "l",
      "typeScale": "1.25"
    }
  ]
}
